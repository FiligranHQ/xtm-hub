{
  "name": "portal-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"relay-compiler\" \"next dev -p 3001\"",
    "build": "relay-compiler && next build",
    "check-ts": "tsc --noEmit",
    "start": "next start",
    "lint": "next lint",
    "relay": "relay-compiler",
    "prettier": "prettier . --write",
    "test": "vitest"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.4",
    "@svgr/webpack": "8.1.0",
    "@types/node": "20.12.7",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "filigran-icon": "0.5.0",
    "filigran-ui": "0.5.1",
    "graphql": "16.8.1",
    "graphql-sse": "2.5.3",
    "next": "14.2.3",
    "postcss": "8.4.38",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.51.3",
    "react-relay": "16.2.0",
    "tailwind-merge": "2.3.0",
    "tailwindcss": "3.4.3",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.4.5",
    "zod": "3.23.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "@testing-library/react": "^15.0.7",
    "@types/config": "3.3.4",
    "@types/react-relay": "16.0.6",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "jsdom": "^24.0.0",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.14",
    "relay-compiler": "16.2.0",
    "vitest": "^1.6.0"
  },
  "packageManager": "yarn@3.6.3",
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "bracketSameLine": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "singleAttributePerLine": true,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
