{
  "name": "portal-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"relay-compiler\" \"next dev -p 3001\"",
    "build": "relay-compiler && next build",
    "check-ts": "tsc --noEmit",
    "start": "next start",
    "lint": "next lint",
    "relay": "relay-compiler",
    "format": "prettier . --write",
    "test": "vitest run",
    "test:w": "vitest",
    "test:e2e": "yarn playwright test",
    "test:e2e:ui": "yarn playwright test --ui",
    "generate-test-e2e": "npx playwright codegen http://localhost:3001/",
    "prepare": "cd .. && husky ./.husky"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@svgr/webpack": "8.1.0",
    "@types/node": "20.12.7",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "filigran-icon": "0.5.0",
    "filigran-ui": "0.7.1",
    "graphql": "16.8.1",
    "graphql-sse": "2.5.3",
    "next": "14.2.3",
    "postcss": "8.4.38",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.51.3",
    "react-relay": "16.2.0",
    "tailwind-merge": "2.3.0",
    "tailwindcss": "3.4.3",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.4.5",
    "zod": "3.23.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/config": "3.3.4",
    "@types/react-relay": "16.0.6",
    "@types/relay-test-utils": "^14.1.4",
    "@vitejs/plugin-react": "^4.3.0",
    "babel-plugin-relay": "16.2.0",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.5",
    "monocart-reporter": "^2.4.7",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.14",
    "relay-compiler": "16.2.0",
    "relay-test-utils": "^16.2.0",
    "vite-plugin-relay": "^2.1.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "packageManager": "yarn@3.6.3",
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "bracketSameLine": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "singleAttributePerLine": true,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
