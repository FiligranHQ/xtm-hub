{
  "name": "portal-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"relay-compiler\" \"next dev -p 3001\"",
    "build": "relay-compiler && next build",
    "check-ts": "tsc --noEmit",
    "start": "next start",
    "lint": "next lint",
    "relay": "relay-compiler",
    "format": "prettier . --write",
    "test": "vitest run",
    "test:w": "vitest",
    "test:e2e": "yarn playwright test",
    "test:e2e:ui": "yarn playwright test --ui",
    "generate-test-e2e": "npx playwright codegen http://localhost:3001/",
    "prepare": "cd .. && husky ./.husky"
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.0",
    "@svgr/webpack": "8.1.0",
    "autoprefixer": "10.4.20",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "extract-files": "13.0.0",
    "filigran-icon": "0.8.1",
    "filigran-ui": "0.18.1",
    "graphql": "16.9.0",
    "graphql-sse": "2.5.3",
    "next": "14.2.12",
    "postcss": "8.4.47",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.53.0",
    "react-relay": "17.0.0",
    "tailwind-merge": "2.5.2",
    "tailwindcss": "3.4.12",
    "tailwindcss-animate": "1.0.7",
    "usehooks-ts": "3.1.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "1.47.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/config": "3.3.5",
    "@types/node": "22.5.5",
    "@types/react": "18.3.7",
    "@types/react-dom": "18.3.0",
    "@types/react-relay": "16.0.6",
    "@types/relay-test-utils": "17.0.0",
    "@vitejs/plugin-react": "4.3.1",
    "babel-plugin-relay": "17.0.0",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.12",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.1.6",
    "jsdom": "24.1.1",
    "lint-staged": "15.2.10",
    "monocart-reporter": "2.8.1",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.6",
    "relay-compiler": "17.0.0",
    "relay-test-utils": "17.0.0",
    "typescript": "5.6.2",
    "vite-plugin-relay": "2.1.0",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.1.1"
  },
  "packageManager": "yarn@3.6.3",
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "bracketSameLine": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "singleAttributePerLine": true,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
