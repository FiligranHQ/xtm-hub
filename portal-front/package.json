{
  "name": "portal-front",
  "version": "1.4.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"relay-compiler\" \"next dev  --turbopack -p 3002\"",
    "build": "relay-compiler && next build",
    "check-ts": "tsc --noEmit",
    "check-ts-test": "tsc --project tsconfig.test.json",
    "start": "next start -p 3002",
    "lint": "next lint",
    "relay": "relay-compiler",
    "format": "prettier . --write",
    "test": "vitest run",
    "test:w": "vitest",
    "test:ci": "yarn test && yarn lint",
    "prepare": "cd .. && husky ./.husky",
    "auto-translation": "i18n-auto-translation -a deepl-free -p ./messages/en.json -t fr -k $SUBSCRIPTION_KEY"
  },
  "dependencies": {
    "@hookform/resolvers": "3.10.0",
    "@svgr/webpack": "8.1.0",
    "@uiw/react-md-editor": "4.0.5",
    "autoprefixer": "10.4.20",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "extract-files": "13.0.0",
    "filigran-icon": "0.14.1",
    "filigran-ui": "0.37.0",
    "graphql": "16.10.0",
    "graphql-sse": "2.5.4",
    "lucide-react": "0.471.1",
    "next": "15.2.3",
    "next-intl": "3.26.3",
    "next-themes": "0.4.4",
    "postcss": "8.4.49",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.52.0",
    "react-markdown": "10.1.0",
    "react-relay": "18.2.0",
    "slugify": "1.6.6",
    "tailwind-merge": "2.6.0",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "1.0.7",
    "usehooks-ts": "3.1.0",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@playwright/test": "1.49.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/config": "3.3.5",
    "@types/node": "22.10.5",
    "@types/react": "19.0.6",
    "@types/react-dom": "19.0.3",
    "@types/react-relay": "18.2.0",
    "@types/relay-test-utils": "18.0.0",
    "@vitejs/plugin-react": "4.3.4",
    "babel-plugin-relay": "18.2.0",
    "concurrently": "9.1.2",
    "eslint": "9.18.0",
    "eslint-config-next": "15.1.4",
    "eslint-config-prettier": "10.0.1",
    "husky": "9.1.7",
    "i18n-auto-translation": "1.10.0",
    "jsdom": "26.0.0",
    "lint-staged": "15.3.0",
    "monocart-reporter": "2.9.13",
    "prettier": "3.4.2",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-tailwindcss": "0.6.9",
    "rehype-sanitize": "6.0.0",
    "relay-compiler": "18.2.0",
    "relay-test-utils": "18.2.0",
    "typescript": "5.7.3",
    "vite-plugin-relay": "2.1.0",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.1.8"
  },
  "packageManager": "yarn@3.6.3",
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "bracketSameLine": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "singleAttributePerLine": true,
    "plugins": [
      "prettier-plugin-tailwindcss",
      "prettier-plugin-organize-imports"
    ]
  }
}
