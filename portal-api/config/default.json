{
  "environment": "development",
  "port": 4001,
  "admin": {
    "email": "admin@filigran.io",
    "password": "admin"
  },
  "database": {
    "host": "127.0.0.1",
    "port": 5434,
    "user": "portal",
    "password": "portal-password",
    "database": "cloud-portal"
  },
  "database-test": {
    "database": "test_database",
    "seeds": "tests/seeds"
  },
  "auth0": {
    "enabled": false,
    "config": {
      "domain": "changeme",
      "clientId": "changeme",
      "clientSecret": "changeme"
    }
  },
  "oidc_provider": {
    "issuer": "http://localhost:8080/realms/keycloak-express",
    "client_id": "keycloak-express",
    "client_secret": "long_secret-here",
    "redirect_uris": ["http://localhost:3001/auth/oidc/callback"],
    "logout_callback_url": ["http://localhost:3001/oidc/callback"],
    "response_types": ["code"]
  },
  "minio": {
    "bucketName": "xtmhubbucket",
    "region": "us-east-1",
    "endpoint": "localhost",
    "port": 9000,
    "accessKeyId": "changeme",
    "secretAccessKey": "changeme",
    "useSsl": false
  },
  "smtp_options": {
    "host": "localhost",
    "port": 1025,
    "secure": false,
    "auth": {
      "user": "changeMe",
      "pass": "changeMe"
    },
    "from": "no-reply@filigran.cloud"
  },
  "login_settings": [
    {
      "name": "local",
      "type": "FORM",
      "provider": "local"
    },
    {
      "name": "Login with Filigran",
      "type": "SSO",
      "provider": "oidc"
    }
  ],

  "user_management": {
    "token_reference": "access_token",
    "read_userinfo": false,
    "roles_path": "resource_access.scred-portal-dev.roles",
    "role_mappings": ["XTMHubAdmin:ADMIN", "User:USER"]
  },
  "awx": {
    "activate": false,
    "url": null,
    "token": null,
    "workflow": null,
    "action_mapping": null
  },
  "base_url_front": "https://dev.hub.staging.filigran.io",
  "app_logs": {
    "logs_level": "info",
    "extended_error_message": true
  }
}
