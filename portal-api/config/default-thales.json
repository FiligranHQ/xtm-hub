{
  "port": 4001,
  "admin": {
    "email": "admin@filigran.io",
    "password": "admin"
  },
  "database": {
    "host": "thales-dev-vm-2",
    "port": 5432,
    "user": "portal",
    "password": "portal-password",
    "database": "cloud-portal"
  },
  "oidc_provider": {
    "issuer": "changeme",
    "client_id": "changeme",
    "client_secret": "changeme",
    "redirect_uris": ["http://localhost:3000/auth/oidc/callback"],
    "logout_callback_url": ["http://localhost:3000/oidc/callback"],
    "response_types": ["code"]
  },
  "init_services": [
    {
      "cyber_weather": {
        "id": "c6343882-f609-4a3f-abe0-a34f8cb11302",
        "name": "CyberWeather",
        "provider": "SCRED6",
        "type": "Intel",
        "description": "This is a short description to describe the CyberWeather app, how to use it, etc.",
        "url": "https://weather.dev.scredplatform.io/",
        "subscription_type": "SUBSCRIPTABLE_DIRECT"
      }
    },
    {
      "open_feed": {
        "id": "d6343883-f609-5a3f-ace1-a24f8cb11454",
        "name": "OpenFeed",
        "provider": "SCRED5",
        "type": "Feed",
        "description": "This is a short description to describe the OpenFeed app, how to use it, etc.",
        "url": "https://opencti-test2.non-prod.scredplatform.io/",
        "subscription_type": "SUBSCRIPTABLE_DIRECT"
      }
    }
  ],
  "user_management": {
    "token_reference": "access_token",
    "read_userinfo": false,
    "roles_path": "realm_access.roles",
    "role_mappings": [
      "portal-admin:ADMIN",
      "organization-user:USER",
      "organization-admin:ADMIN_ORGA"
    ]
  },
  "awx": {
    "activate": true,
    "url": "changeme",
    "token": "changeme",
    "workflow": "/api/v2/workflow_job_templates/${workflowId}/launch/",
    "action_mapping": {
      "CREATE_USER": {
        "path": "/api/v2/workflow_job_templates/?name=api_create-user",
        "keys": ["roles", "organization_name"]
      },
      "DISABLE_USER": {
        "path": "/api/v2/workflow_job_templates/?name=api_disable-user",
        "keys": ["roles", "organization_name"]
      },
      "CREATE_COMMUNITY_USER": {
        "path": "/api/v2/workflow_job_templates/?name=api_create-community",
        "keys": ["roles", "organization_name"]
      },
      "DELETE_COMMUNITY_USER": {
        "path": "/api/v2/workflow_job_templates/?name=api_delete-community",
        "keys": ["roles", "organization_name"]
      },
      "COMMUNITY_ADD_USER": {
        "path": "/api/v2/workflow_job_templates/?name=api_community-add-user",
        "keys": ["roles", "organization_name"]
      },
      "COMMUNITY_REMOVE_USER": {
        "path": "/api/v2/workflow_job_templates/?name=api_community-remove-user",
        "keys": ["roles", "organization_name"]
      }
    }
  }
}
