{
  "name": "portal-front",
  "version": "1.4.8",
  "private": true,
  "scripts": {
    "dev": "concurrently \"relay-compiler && tsx scripts/generate-enum.ts\" \"next dev  --turbopack -p 3002\"",
    "build": "relay-compiler && tsx scripts/generate-enum.ts && next build",
    "check-ts": "tsc --noEmit",
    "check-ts-test": "tsc --project tsconfig.test.json",
    "start": "next start -p 3002",
    "lint": "next lint",
    "relay": "relay-compiler && tsx scripts/generate-enum.ts",
    "format": "prettier . --write",
    "test": "vitest run",
    "test:w": "vitest",
    "test:ci": "yarn test && yarn lint",
    "prepare": "cd .. && husky ./.husky",
    "auto-translation": "i18n-auto-translation -a deepl-free -p ./messages/en.json -t fr -k $SUBSCRIPTION_KEY",
    "generate:enum": "tsx scripts/generate-enum.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "3.10.0",
    "@nx/devkit": "21.2.2",
    "@nx/js": "21.2.2",
    "@nx/next": "^21.2.2",
    "@svgr/webpack": "8.1.0",
    "@testing-library/react": "^16.3.0",
    "@uiw/react-md-editor": "4.0.7",
    "autoprefixer": "10.4.21",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "extract-files": "13.0.0",
    "file-loader": "6.2.0",
    "filigran-icon": "0.15.0",
    "filigran-ui": "0.38.1",
    "graphql": "16.11.0",
    "graphql-sse": "2.5.4",
    "lucide-react": "0.471.1",
    "next-intl": "4.1.0",
    "next-themes": "0.4.6",
    "postcss": "8.5.3",
    "react-hook-form": "7.52.0",
    "react-markdown": "10.1.0",
    "rehype-sanitize": "^6.0.0",
    "semver": "6.3.1",
    "sharp": "0.34.3",
    "slugify": "1.6.6",
    "tailwind-merge": "2.6.0",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.8.3",
    "zod": "3.24.2"
  },
  "peerDependencies": {
    "@types/node": "24.0.12",
    "@types/react": "19.1.6",
    "@types/react-relay": "18.2.1",
    "next": "15.3.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-relay": "18.2.0",
    "tsx": "4.19.4",
    "usehooks-ts": "3.1.0",
    "vitest": "3.2.4"
  },
  "packageManager": "yarn@3.8.7",
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "bracketSameLine": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "singleAttributePerLine": true,
    "plugins": [
      "prettier-plugin-tailwindcss",
      "prettier-plugin-organize-imports"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/testing-library__jest-dom": "^6.0.0"
  }
}
