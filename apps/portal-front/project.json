{
  "name": "portal-front",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/portal-front/src",
  "projectType": "application",
  "types": ["next", "yarn"],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/portal-front",
        "command": "yarn dev"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "{projectRoot}/dist",
        "cwd": "apps/portal-front",
        "command": "graphql-codegen && relay-compiler && tsx scripts/generate-enum.ts && next build"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/portal-front",
        "command": "next start -p 3002"
      }
    }
  },
  "tags": []
}
